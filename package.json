{
  "name": "element-identifier",
  "version": "0.1.0",
  "description": "Generate stable, unique CSS selectors to identify DOM elements.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "example:react-counter": "node scripts/serve-react-counter.js",
    "prepublishOnly": "npm run build && npm test",
    "release:patch": "npm version patch && git push --tags",
    "release:minor": "npm version minor && git push --tags",
    "release:major": "npm version major && git push --tags"
  },
  "keywords": ["typescript", "library", "npm", "dom", "css", "selector", "identify", "element"],
  "author": "Joo Herrera <jooherrera@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jooherrera/element-identifier.git"
  },
  "bugs": {
    "url": "https://github.com/jooherrera/element-identifier/issues"
  },
  "homepage": "https://github.com/jooherrera/element-identifier#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "jest-environment-jsdom": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rollup": "^4.21.0",
    "@rollup/plugin-terser": "^0.4.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4",
    "tslib": "^2.6.3"
  }
}
